{
  "name": "lazy-transform",
  "version": "0.0.1",
  "description": "Transform iterable into another iterable lazily",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/nieltg/lazy-transform-js",
  "author": "nieltg",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/is-callable": "^1.1.0",
    "@types/mocha": "^5.2.3",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "is-callable": "^1.1.4",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.5",
    "source-map-support": "^0.5.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm --verbose -r dist || true",
    "lint": "tslint -p . --fix",
    "precommit": "lint-staged",
    "prepublish": "$npm_execpath run build",
    "test": "nyc mocha"
  },
  "lint-staged": {
    "lib/**/*.{js,ts}": [
      "tslint -p . --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist"
    ],
    "all": true
  },
  "dependencies": {}
}
